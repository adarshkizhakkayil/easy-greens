    <section>
        <div class="container wallet-container mt-5 mb-4">
            <div class="col-md-12">
                <div class="text-center rounded p-4 " style="background-color: #f3f2ee;">
                    <div class="d-flex align-items-center justify-content-between mb-4">
                        <h6 class="checkout__title mb-0">wallet History</h6>
                        <button id="showAllBtn" class="btn btn-light btn-sm">Show All</button>
                        <button id="showLessBtn" class="btn btn-light btn-sm" style="display: none;">Show Less</button>
                    </div>
                    <% if (walletdata && walletdata.wallet.walletHistory.length> 0) { %>

                        <% walletdata.wallet.walletHistory.forEach((transaction,index)=> {%>
                            <ul class="transaction-list d-flex align-items-center">
                                <!-- Transaction History Items -->
                                <li
                                    class="transaction-item d-flex justify-content-between w-100 additional-transaction">
                                    <span class="transaction-date">
                                        <%=transaction.date.toLocaleString()%>
                                    </span>
                                </li>
                                <li
                                    class="transaction-item d-flex justify-content-between w-100 additional-transaction">
                                <li
                                    class="transaction-item d-flex justify-content-between w-100 additional-transaction">
                                    <span class="reason">reason : <%=transaction.reason%></span>
                                </li>
                                <li
                                    class="transaction-item d-flex justify-content-between w-100 additional-transaction">
                                    <span class="<%=transaction.type%>">
                                        <%= transaction.type=='credit' ? 'credited' : 'debited' %>
                                    </span>
                                </li>
                                <li
                                    class="transaction-item d-flex justify-content-between w-100 additional-transaction">
                                    <span class="amount <%=transaction.type%>">
                                        <%= transaction.type=='credit' ? '+' : '-' %>
                                            <%= transaction.amount %>
                                    </span>
                                </li>
                                <!-- Add more transactions as needed -->
                            </ul>
                            <% }) %>
                                <% } else { %>
                                    <p class="text-danger">No wallet history found!</p>
                                    <% } %>
                </div>
                <div class="text-center rounded p-4 mt-4" style="background-color: #f3f2ee;">
                    <div class="checkout__title d-flex align-items-center justify-content-between mb-4">
                        <h6 class="mb-0">Balance</h6>
                    </div>
                    <div class="text-uppercase font-awesome font-weight-bold">
                        Balance : ₹ <%=walletdata?.wallet?.walletAmount%>
                    </div>
                </div>
            </div>
        </div>
    </section>
